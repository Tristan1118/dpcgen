{
  "manifest_version": 2,
  "name": "DPCGen",
  "version": "1.0",
  "permissions": [
    "tabs", "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "window.html",
    "testcases/*"
  ]
}
