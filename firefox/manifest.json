{
  "manifest_version": 2,
  "name": "DPCGen",
  "version": "1.0",
  "permissions": [
    "tabs", "storage", "activeTab"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "window.html",
    "testcases/*",
    "checklist.html"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["checklist.js"]
    }
 ]
}
